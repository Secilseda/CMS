@model IEnumerable<AppUser>
@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div class="col-sm-12">
		<div class="card">
			<div class="card-header">
				<h5>List of Users</h5>
			</div>
			<div class="card-body">
				<div class="table table-responsive">
					<table class="table table-striped">
						<thead>
							<tr>
								<th>Id</th>
								<th>Name</th>
								<th>Email</th>
							</tr>
						</thead>
						<tbody>
							@foreach (var item in Model)
							{
								<tr>
									<td>@item.Id</td>
									<td>@item.UserName</td>
									<td>@item.Email</td>
								</tr>
							}
						</tbody>
					</table>
				</div>
			</div>
			<!--footor.-->
		</div>
    </div>
</div>

